    "FROM library/python:3.6.3-alpine",
    "RUN apk add zlib libjpeg-turbo-dev libpng-dev freetype-dev lcms2-dev libwebp-dev harfbuzz-dev fribidi-dev tcl-dev tk-dev",
    "RUN mkdir -p /usr/src/app",
    "WORKDIR /usr/src/app",
